{"ast":null,"code":"import AdventureService from \"@/services/AdventureService\";\nexport default {\n  name: \"simple-adventure\",\n  components: {},\n  props: ['adventure', 'path'],\n  methods: {\n    goToDetailAdventure() {\n      console.log(this.adventure.id);\n      this.$router.push('/adventures/detail/' + this.adventure.id);\n    },\n\n    goToUpdateAdventure() {\n      console.log(this.adventure.id);\n      this.$router.push('/adventure/update/' + this.adventure.id);\n    },\n\n    deleteAdventure() {\n      console.log(this.adventure.id + \"  za delete\");\n      AdventureService.deleteAdventure(this.adventure.id).then(response => {\n        console.log(response);\n        this.$emit('rerender');\n      }).catch(function (error) {\n        console.log(error.toJSON());\n\n        if (error.response) {\n          // The request was made and the server responded with a status code\n          // that falls out of the range of 2xx\n          console.log(error.response.data);\n          console.log(error.response.status);\n          console.log(error.response.headers);\n        } else if (error.request) {\n          // The request was made but no response was received\n          // error.request is an instance of XMLHttpRequest in the browser and an instance of\n          // http.ClientRequest in node.js\n          console.log(error.request);\n        } else {\n          // Something happened in setting up the request that triggered an Error\n          console.log('Error', error.message);\n        }\n\n        console.log(error.config);\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      adventures: []\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,gBAAP,MAA6B,6BAA7B;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;AAEbC,YAAU,EAAE,EAFC;AAGbC,OAAK,EAAE,CAAC,WAAD,EAAc,MAAd,CAHM;AAIbC,SAAO,EAAE;AACPC,uBAAmB,GAAG;AACpBC,aAAO,CAACC,GAAR,CAAY,KAAKC,SAAL,CAAeC,EAA3B;AACA,WAAKC,OAAL,CAAaC,IAAb,CAAkB,wBAAwB,KAAKH,SAAL,CAAeC,EAAzD;AACD,KAJM;;AAMPG,uBAAmB,GAAG;AACpB