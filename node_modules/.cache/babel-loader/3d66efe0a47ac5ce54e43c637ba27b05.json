{"ast":null,"code":"import cottageServce from \"@/servieces/CottageServce\";\nimport SiteCottageElement from \"@/components/main_site/siteCottageElement\";\nexport default {\n  name: \"cottageList\",\n  components: {\n    SiteCottageElement\n  },\n\n  mounted() {\n    cottageServce.getCottages().then(response => {\n      this.cottages = response.data;\n    });\n  },\n\n  methods: {\n    filterOffers() {\n      this.resultingOffers = [];\n      let index = 0;\n\n      for (let offer of this.cottages) {\n        if (this.checkIfNeedsToBeDisplayed(index)) {\n          this.resultingOffers.push(offer);\n        }\n\n        index++;\n      }\n\n      console.log(this.resultingOffers);\n      return this.resultingOffers;\n    },\n\n    makeMiddleNumber() {\n      let numPages = this.cottages.length / this.numberOfElementsForDisplay;\n      return Math.ceil(numPages / 2);\n    },\n\n    makeLastNumber() {\n      let numPages = this.cottages.length / this.numberOfElementsForDisplay;\n      return Math.round(numPages);\n    },\n\n    makeFirstLink() {\n      this.fromElement = 0;\n      this.forceRemounting();\n    },\n\n    makeMiddleLink() {\n      this.fromElement = (this.makeMiddleNumber() - 1) * this.numberOfElementsForDisplay;\n      this.forceRemounting();\n    },\n\n    makeLastLink() {\n      this.fromElement = (this.makeLastNumber() - 1) * this.numberOfElementsForDisplay;\n      this.forceRemounting();\n    },\n\n    makePreviousLink() {\n      let previousFrom = parseInt(this.fromElement) - parseInt(this.numberOfElementsForDisplay);\n      if (previousFrom < 0) return;\n      this.fromElement = previousFrom;\n      this.forceRemounting();\n    },\n\n    makeNextLink() {\n      let nextFrom = parseInt(this.fromElement) + parseInt(this.numberOfElementsForDisplay);\n      if (nextFrom > parseInt(this.cottageReservationHistory.length)) return;\n      this.fromElement = nextFrom;\n      this.forceRemounting();\n    },\n\n    checkIfNeedsToBeDisplayed(index) {\n      let untilElement = parseInt(this.numberOfElementsForDisplay) + parseInt(this.fromElement);\n      console.log(index);\n      console.log(this.fromElement);\n      console.log(untilElement);\n\n      if (index >= this.fromElement && index < untilElement) {\n        return true;\n      }\n\n      return false;\n    }\n\n  },\n\n  data() {\n    return {\n      cottages: [],\n      numberOfElementsForDisplay: 2\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,aAAP,MAA0B,2BAA1B;AACA,OAAOC,kBAAP,MAA+B,2CAA/B;AACA,eAAe;AACbC,MAAI,EAAE,aADO;AAEbC,YAAU,EAAE;AAACF;AAAD,GAFC;;AAGbG,SAAO,GAAG;AACRJ,iBAAa,CAACK,WAAd,GAA4BC,IAA5B,CACKC,QAAD,IAAc;AACZ,WAAKC,QAAL,GAAgBD,QAAQ,CAACE,IAAzB;AACF,KAHJ;AAKD,GATY;;AAUbC,SAAO,EAAE;AACPC,gBAAY,GAAE;AACZ,WAAKC,eAAL,GAAuB,EAAvB;AACA,UAAIC,KAAI,GAAI,CAAZ;;AACA,WAAK,IAAIC,KAAT,IAAkB,KAAKN,QAAvB,EAAgC;AAC9B,YAAI,KAAKO,yBAAL,CAA+BF,KAA/B,CAAJ,EAA2C;AACzC,eAAKD,eAAL,CAAqBI,IAArB,CAA0BF,KAA1B;AACF;;AACAD,aAAK;AACP;;AACAI,aAAO,CAACC,GAAR,CAAY,KAAKN,eAAjB;AACA,aAAO,KAAKA,eAAZ;AACD,KAZM;;AAaPO,oBAAgB,GAAG;AACjB,UAAIC,QAAO,GAAI,KAAKZ,QAAL,CAAca,MAAd,GAAuB,KAAKC,0BAA3C;AACA,aAAOC,IAAI,CAACC,IAAL,CAAUJ,QAAO,GAAI,CAArB,CAAP;AACD,KAhBM;;AAiBPK,kBAAc,GAAG;AACf,UAAIL,QAAO,GAAI,KAAKZ,QAAL,CAAca,MAAd,GAAuB,KAAKC,0BAA3C;AACA,aAAOC,IAAI,CAACG,KAAL,CAAWN,QAAX,CAAP;AACD,KApBM;;AAqBPO,iBAAa,GAAG;AACd,WAAKC,WAAL,GAAmB,CAAnB;AACA,WAAKC,eAAL;AACD,KAxBM;;AAyBPC,kBAAc,GAAG;AACf,WAAKF,WAAL,GAAmB,CAAC,KAAKT,gBAAL,KAA0B,CAA3B,IAAgC,KAAKG,0BAAxD;AACA,WAAKO,eAAL;AACD,KA5BM;;AA6BPE,gBAAY,GAAG;AACb,WAAKH,WAAL,GAAmB,CAAC,KAAKH,cAAL,KAAwB,CAAzB,IAA8B,KAAKH,0BAAtD;AACA,WAAKO,eAAL;AACD,KAhCM;;AAiCPG,oBAAgB,GAAG;AACjB,UAAIC,YAAW,GAAIC,QAAQ,CAAC,KAAKN,WAAN,CAAR,GAA6BM,QAAQ,CAAC,KAAKZ,0BAAN,CAAxD;AACA,UAAIW,YAAW,GAAI,CAAnB,EACE;AACF,WAAKL,WAAL,GAAmBK,YAAnB;AACA,WAAKJ,eAAL;AACD,KAvCM;;AAwCPM,gBAAY,GAAG;AACb,UAAIC,QAAO,GAAIF,QAAQ,CAAC,KAAKN,WAAN,CAAR,GAA6BM,QAAQ,CAAC,KAAKZ,0BAAN,CAApD;AACA,UAAIc,QAAO,GAAIF,QAAQ,CAAC,KAAKG,yBAAL,CAA+BhB,MAAhC,CAAvB,EACE;AACF,WAAKO,WAAL,GAAm