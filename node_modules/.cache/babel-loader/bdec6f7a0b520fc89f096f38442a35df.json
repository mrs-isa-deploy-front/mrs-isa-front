{"ast":null,"code":"import SiteShipList from \"@/components/main_site/siteShipList\";\nimport BasicHeader from \"@/components/main_site/basicHeader\";\nimport ClientHeader from \"@/components/client/clientHeader\";\nimport SiteShipSearchNav from \"@/components/main_site/siteShipSearchNav\";\nimport shipServce from \"@/servieces/ShipServce\";\nimport cottageServce from \"@/servieces/CottageServce\";\nexport default {\n  name: \"siteShipListView\",\n  components: {\n    SiteShipSearchNav,\n    ClientHeader,\n    BasicHeader,\n    SiteShipList\n  },\n  created: function () {\n    shipServce.getShips().then(response => {\n      this.cottages = response.data;\n      this.listLength = this.cottages.length;\n    });\n\n    try {\n      if (JSON.parse(localStorage.user) == null) {\n        this.verifiedClient = false;\n      } else {\n        this.verifiedClient = true;\n      }\n    } catch (error) {\n      this.verifiedClient = false;\n    }\n\n    this.forceRemounting();\n  },\n  methods: {\n    sortList() {\n      let sortBy = document.getElementById(\"sortBy\").value;\n\n      if (sortBy == 1) {\n        cottageServce.sortShipListByName(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      } else if (sortBy == 2) {\n        cottageServce.sortShipListByLocation(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      } else if (sortBy == 3) {\n        cottageServce.sortShipListByRating(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      } else if (sortBy == 4) {\n        cottageServce.sortShipListByPrice(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      } else if (sortBy == 5) {\n        cottageServce.sortShipListBySpeed(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      } else if (sortBy == 6) {\n        cottageServce.sortShipListByCapacity(this.cottages).then(response => {\n          this.cottages = response.data;\n          this.listLength = this.cottages.length;\n          this.forceRemounting();\n        });\n      }\n    },\n\n    filterShips(filterDto) {\n      shipServce.filterShips(filterDto).then(response => {\n        this.cottages = response.data;\n        this.listLength = this.cottages.length;\n        this.shipsKey++;\n        console.log(this.listLength);\n      });\n    },\n\n    forceRemounting() {\n      this.clientHeaderKey += 1;\n      this.basicHeaderKey += 1;\n      this.shipsKey += 1;\n    }\n\n  },\n\n  data() {\n    return {\n      cottages: [],\n      listLength: 0,\n      verifiedClient: false,\n      basicHeaderKey: 0,\n      clientHeaderKey: 0,\n      shipsKey: 1\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,YAAP,MAAyB,qCAAzB;AACA,OAAOC,WAAP,MAAwB,oCAAxB;AACA,OAAOC,YAAP,MAAyB,kCAAzB;AACA,OAAOC,iBAAP,MAA8B,0CAA9B;AACA,OAAOC,UAAP,MAAuB,wBAAvB;AACA,OAAOC,aAAP,MAA0B,2BAA1B;AACA,eAAe;AACbC,MAAI,EAAE,kBADO;AAEbC,YAAU,EAAE;AAACJ,qBAAD;AAAoBD,gBAApB;AAAkCD,eAAlC;AAA+CD;AAA/C,GAFC;AAGbQ,SAAO,EACH,YAAY;AACVJ,cAAU,CAACK,QAAX,GAAsBC,IAAtB,CACKC,QAAD,IAAc;AACZ,WAAKC,QAAL,GAAgBD,QAAQ,CAACE,IAAzB;AACA,WAAKC,UAAL,GAAkB,KAAKF,QAAL,CAAcG,MAAhC;AACF,KAJJ;;AAMA,QAAG;AAED,UAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,IAAxB,KAAiC,IAArC,EAA2C;AACzC,aAAKC,cAAL,GAAsB,KAAtB;AACF,OAFA,MAEO;AACL,aAAKA,cAAL,GAAsB,IAAtB;AACF;AACD,KAPD,CAOC,OAAOC,KAAP,EAAa;AACZ,WAAKD,cAAL,GAAsB,KAAtB;AAEF;;AACA,SAAKE,eAAL;AAGD,GAzBQ;AA0BbC,SAAO,EAAE;AACPC,YAAQ,GAAE;AACR,UAAIC,MAAK,GAAIC,QAAQ,CAACC,cAAT,CA