ces/ReservationServce\";\r\nexport default {\r\n  name: \"siteCottageListView\",\r\n  components: {ClientHeader, BasicHeader, SiteCottageList, SiteCottageSearchNav},\r\n  created:\r\n      function () {\r\n        cottageServce.getCottages().then(\r\n            (response) => {\r\n              this.cottages = response.data;\r\n              this.listLength = this.cottages.length;\r\n            }\r\n        )\r\n        try{\r\n\r\n          if (JSON.parse(localStorage.user) == null) {\r\n            this.verifiedClient = false;\r\n          } else {\r\n            this.verifiedClient = true;\r\n          }\r\n        }catch (error){\r\n          this.verifiedClient = false;\r\n\r\n        }\r\n        this.forceRemounting();\r\n\r\n\r\n      },\r\n  methods: {\r\n    sortList(){\r\n      if (sortBy === 1){\r\n        reservationServce.sortCottageListByName(JSON.parse(localStorage.user).id)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      } else if (sortBy === 2){\r\n        reservationServce.sortCottageListByLocation(JSON.parse(localStorage.user).id)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      }else if (sortBy === 3){\r\n        reservationServce.sortCottageListByRating(this.cottages)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      }else if (sortBy === 4){\r\n        reservationServce.sortCottageListByPrice(this.cottages)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      }else if (sortBy === 5){\r\n        reservationServce.sortCottageListByNumberOfRooms(this.cottages)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      }else if (sortBy === 6){\r\n        reservationServce.sortCottageListByNumberOfBeds(this.cottages)\r\n            .then(response =>{\r\n              this.cottageReservationHistory = response.data;\r\n              this.forceRemounting();\r\n            })\r\n      }\r\n    },\r\n    filterCottages(filterDto){\r\n      console.log(filterDto);\r\n      cottageServce.filterCottages(filterDto)\r\n          .then((response) => {\r\n              this.cottages = response.data;\r\n              this.listLength = this.cottages.length;\r\n              this.cottagesKey++;\r\n              console.log(this.listLength);\r\n              }\r\n          )\r\n    },\r\n    forceRemounting() {\r\n      this.clientHeaderKey += 1;\r\n      this.basicHeaderKey += 1;\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      cottages: [],\r\n      listLength: 0,\r\n      verifiedClient: false,\r\n      basicHeaderKey: 0,\r\n      clientHeaderKey: 0,\r\n      cottagesKey: 1\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}