 } else if (sortBy === 5) {\n        reservationServce.sortCottageListByNumberOfRooms(this.cottages).then(response => {\n          this.cottageReservationHistory = response.data;\n          this.forceRemounting();\n        });\n      } else if (sortBy === 6) {\n        reservationServce.sortCottageListByNumberOfBeds(this.cottages).then(response => {\n          this.cottageReservationHistory = response.data;\n          this.forceRemounting();\n        });\n      }\n    },\n\n    filterCottages(filterDto) {\n      console.log(filterDto);\n      cottageServce.filterCottages(filterDto).then(response => {\n        this.cottages = response.data;\n        this.listLength = this.cottages.length;\n        this.cottagesKey++;\n        console.log(this.listLength);\n      });\n    },\n\n    forceRemounting() {\n      this.clientHeaderKey += 1;\n      this.basicHeaderKey += 1;\n    }\n\n  },\n\n  data() {\n    return {\n      cottages: [],\n      listLength: 0,\n      verifiedClient: false,\n      basicHeaderKey: 0,\n      clientHeaderKey: 0,\n      cottagesKey: 1\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,oBAAP,MAAiC,6CAAjC;AACA,OAAOC,eAAP,MAA4B,wCAA5B;AACA,OAAOC,WAAP,MAAwB,oCAAxB;AACA,OAAOC,YAAP,MAAyB,kCAAzB;AACA,OAAOC,aAAP,MAA0B,2BAA1B;AACA,OAAOC,iBAAP,MAA8B,+BAA9B;AACA,eAAe;AACbC,MAAI,EAAE,qBADO;AAEbC,YAAU,EAAE;AAACJ,gBAAD;AAAeD,eAAf;AAA4BD,mBAA5B;AAA6CD;AAA7C,GAFC;AAGbQ,SAAO,EACH,YAAY;AACVJ,iBAAa,CAACK,WAAd,GAA4BC,IAA5B,CACKC,QAAD,IAAc;AACZ,WAAKC,QAAL,GAAgBD,QAAQ,CAACE,IAAzB;AACA,WAAKC,UAAL,GAAkB,KAAKF,QAAL,CAAcG,MAAhC;AACF,KAJJ;;AAMA,QAAG;AAED,UAAIC,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,IAAxB,KAAiC,IAArC,EAA2C;AACzC,aAAKC,cAAL,GAAsB,KAAtB;AACF,OAFA,MAEO;AACL,aAAKA,cAAL,GAAsB,IAAtB;AACF;AACD,KAPD,CAOC,OAAOC,KAAP,EAAa;AACZ,WAAKD,cAAL,GAAsB,KAAtB;AAEF;;AACA,SAAKE,eAAL;AAGD,GAzBQ;AA0BbC,SAAO,EAAE;AACPC,YAAQ,GAAE;AACR,UAAIC,MAAK,KAAM,CAAf,EAAiB;AACfpB,yBAAiB,CAACqB,qBAAlB,CAAwCV,IAAI,CAACC,KAAL,CAAWC,YAAY,CAACC,IAAxB,EAA8BQ,EAAtE,EACKjB,IADL,CACUC,QAAO,IAAI;AACf