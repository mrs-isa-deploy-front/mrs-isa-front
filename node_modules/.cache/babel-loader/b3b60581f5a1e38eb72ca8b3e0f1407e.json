{"ast":null,"code":"import $ from \"jquery\";\nexport default {\n  name: \"inputTextModal\",\n  props: [\"index\", \"header\", \"requestId\", \"btnId\"],\n  created: function () {\n    this.modalId = \"#\" + this.index;\n    console.log(this.index);\n    console.log(this.modalId);\n    console.log(this.requestId);\n  },\n  methods: {\n    openForm() {\n      console.log(\"forma\"); //    $('#'+this.index).modal('show');\n\n      var modalToggle = document.getElementById(this.index); ///myModal.show(modalToggle)\n\n      $('#' + this.index).show(modalToggle);\n      $('#' + this.index).focus(modalToggle);\n    },\n\n    Reject() {\n      if (this.text === \"\") {\n        alert(\"Unesite obrazloženje.\");\n      } else {\n        const modal = document.getElementById(this.index);\n        modal.classList.remove('show');\n        modal.setAttribute('aria-hidden', 'true');\n        modal.setAttribute('style', 'display: none');\n        const modalBackdrops = document.getElementsByClassName('modal-backdrop'); // remove opened modal backdrop\n\n        document.body.removeChild(modalBackdrops[0]);\n        document.body.style.overflow = 'auto';\n        this.$emit('reject-text', this.requestId, this.text);\n      }\n    }\n\n  },\n\n  data() {\n    return {\n      modalId: \"\",\n      text: \"\",\n      errorMessage: \"Unesite obrazloženje.\"\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,CAAP,MAAc,QAAd;AAEA,eAAe;AACbC,MAAI,EAAE,gBADO;AAEbC,OAAK,EAAC,CAAC,OAAD,EAAS,QAAT,EAAmB,WAAnB,EAAgC,OAAhC,CAFO;AAGbC,SAAO,EACH,YAAY;AACV,SAAKC,OAAL,GAAe,MAAI,KAAKC,KAAxB;AACAC,WAAO,CAACC,GAAR,CAAY,KAAKF,KAAjB;AACAC,WAAO,CAACC,GAAR,CAAY,KAAKH,OAAjB;AACAE,WAAO,CAACC,GAAR,CAAY,KAAKC,SAAjB;AACD,GATQ;AAUbC,SAAO,EAAC;AACNC,YAAQ,GAAE;AACRJ,aAAO,CAACC,GAAR,CAAY,OAAZ,EADQ,CAEZ;;AACI,UAAII,WAAU,GAAIC,QAAQ,CAACC,cAAT,CAAwB,KAAKR,KAA7B,CAAlB,CAHQ,CAIR;;AACAL,OAAC,CAAC,MAAI,KAAKK,KAAV,CAAD,CAAkBS,IAAlB,CAAuBH,WAAvB;AACAX,OAAC,CAAC,MAAI,KAAKK,KAAV,CAAD,CAAkBU,KAAlB,CAAwBJ,WAAxB;AACD